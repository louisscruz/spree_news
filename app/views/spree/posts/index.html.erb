<div data-hook="news">
  <h1>
    <%= t("posts")  %>
  </h1>
  <ul>
    <% @posts.each do |post| %>
      <li>
        <div>
          <h6>
            <%= post.event_month %>
          </h6>
          <h6>
            <%= post.event_day %>
          </h6>
          <h6>
            <%= post.event_year %>
          </h6>
          <h6>
            <%= post.event_time %>
          </h6>
          <%= link_to post.title, post %>
        </div>
      </li>
    <% end %>
  </ul>
</div>
